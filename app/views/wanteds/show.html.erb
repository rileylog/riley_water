<div class="container">
  <p id="notice">
    <%= notice %>
  </p>
  <p>
    <strong>Area:</strong>
    <%= @wanted.area.name %>
  </p>
  <p>
    <strong>Volume:</strong>
    <%= @wanted.volume %>
  </p>
  <p>
    <strong>Source:</strong>
    <%= @wanted.source %>
  </p>
  <p>
    <strong>Description:</strong>
    <%= @wanted.description %>
  </p>
  <p>
    <strong>Contact Info:</strong>
    <%= @wanted.contact_info %>
  </p>
  <%= link_to 'Edit', edit_wanted_path(@wanted) %> | <%= link_to 'Back', wanteds_path %>
</div>